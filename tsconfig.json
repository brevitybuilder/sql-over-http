{
  "include": ["src/**/*"],
  "compilerOptions": {
    "module": "ESNext",
    "skipLibCheck": true,
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2022",
    "types": ["@cloudflare/workers-types"],
    "strict": false
  }
}
